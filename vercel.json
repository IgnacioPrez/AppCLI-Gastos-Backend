{
    "version": 2,
    "builds": [
        {
            "src": "./app.ts",
            "use": "@vercel/node"
        }
    ],
    "rewrites": [
        {
            "src": "/(.*)",
            "dest": "app.ts"
        }        
    ],
    "headers":[
        {
            "source": "/(.*)",
            "headers": [
              { "key": "Access-Control-Allow-Origin", "value": "*" }
            ]
          }
    ]
    
}
